{
  "name": "createtest",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "http://localhost:5000"
    }
  },
  "_moduleAliases": {
    "utils": "./src/utils",
    "lib": "./src/lib",
    "reducers": "./src/reducers",
    "sagas": "./src/sagas",
    "components": "./src/components",
    "containers": "./src/containers",
    "constants": "./src/constants",
    "api": "./src/api"
  },
  "jest": {
    "moduleNameMapper": {
      "utils": "<rootDir>/src/utils",
      "lib": "<rootDir>/src/lib",
      "reducers": "<rootDir>/src/reducers",
      "sagas": "<rootDir>/src/sagas",
      "components": "<rootDir>/src/components",
      "containers": "<rootDir>/src/containers",
      "constants": "<rootDir>/src/constants",
      "api": "<rootDir>/src/api"
    }
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-preset-env": "1.7.0",
    "compression": "1.7.2",
    "express": "4.16.3",
    "extensible-duck": "1.4.0",
    "ignore-styles": "5.0.1",
    "immer": "1.3.1",
    "loadable-components": "2.2.2",
    "morgan": "1.9.0",
    "preact": "8.2.9",
    "preact-compat": "3.18.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.3.3",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "sass-loader": "7.0.3"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "eslint . --ext .js && react-app-rewired test --env=jsdom",
    "debug": "NODE_ENV=debug npm test",
    "format": "prettier-eslint --write \"{src,server}/**/*.js\" && eslint . --ext .js --fix",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "dev": "NODE_ENV=development nodemon server/index.js",
    "serve": "NODE_ENV=development node server/index.js",
    "deploy": "NODE_ENV=production node server/index.js"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "prettier-eslint --write \"{src,server}/**/*.js\"",
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-imports": "1.5.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "eslint": "4.19.1",
    "eslint-plugin-react": "7.10.0",
    "lint-staged": "7.2.0",
    "module-alias": "2.1.0",
    "pre-commit": "1.2.2",
    "prettier-eslint-cli": "4.7.1",
    "puppeteer": "1.6.0",
    "react-app-rewire-vendor-splitting": "1.3.0",
    "react-app-rewired": "1.5.2",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dashboard": "2.0.0",
    "why-did-you-update": "0.1.1"
  }
}
